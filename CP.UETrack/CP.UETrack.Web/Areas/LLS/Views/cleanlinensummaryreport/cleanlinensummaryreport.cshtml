                                @Html.Partial("~/Views/DashboardCharts/_ExpiryAlert.cshtml")
                            </div>
                        }

                           @if (ViewData.ContainsKey("Simple"))
                        {
                            <div class="col-sm-4">

                                @Html.Partial("~/Views/DashboardCharts/_BERAsset.cshtml")
                            </div>
