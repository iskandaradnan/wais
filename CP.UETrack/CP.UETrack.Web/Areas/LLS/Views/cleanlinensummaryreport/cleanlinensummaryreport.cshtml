                                    <div class="col-sm-4">
                                        @Html.Partial("~/Views/DashboardCharts/_Deduction.cshtml")
                                    </div>
                                    break;

                                case "Asset Classification":
                                    <div class="col-sm-4">
                                        @Html.Partial("~/Views/DashboardCharts/_AssetCategory.cshtml")
                                    </div>
                                    break;

                                case "Asset Category":
                                    <div class="col-sm-4">
                                        @Html.Partial("~/Views/DashboardCharts/_AssetWarranty.cshtml")
                                    </div>
                                    break;

                                case "KPI Target":
                                    <div class="col-sm-4">
                                        @Html.Partial("~/Views/DashboardCharts/_KPITarget.cshtml")
                                    </div>
                                    break;

                                case "Expiry Alert List":
