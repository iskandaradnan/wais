
@{
    ViewBag.Title = "UETrack - Tracking Technician";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .green {
        background-color: #dff0d8;
    }

    .pink {
        background-color: #f2dede;
    }

    .buttonDisabled {
        background-color: #E0E0E0;
    }
</style>
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkqCkh3sb4lVPt4U-DHmnHej5AWYKbtIo&libraries=places"></script>

<div>

    @functions{
        public string GetAntiForgeryToken()
        {
            string cookieToken, formToken;
            AntiForgery.GetTokens(null, out cookieToken, out formToken);
            return cookieToken + ":" + formToken;
        }
    }

    <input id="antiForgeryToken" type="hidden" />
    <input type="hidden" id="primaryID" name="primaryID" value="@ViewBag.CurrentID" />
    <input type="hidden" id="ActionType" name="ActionType" value="@ViewBag.ActionType" />
    <input type="hidden" id="Timestamp" name="Timestamp" value="null" />

    <div class="content_header">
        <ol class="breadcrumb">
            <li><a href="/home/dashboard"><i class="fa fa-home"></i></a></li>
            <li>User Management</li>
            <li class="active">Tracking Technician</li>
        </ol>
        <h4 >Tracking Technician</h4>
        <h6 id="Engineername">Tracking Technician</h6>
       
    </div>

   

    <div class="content_body">
        <form name="frmsmartassign" id="frmsmartassign" class="form-horizontal" novalidate autocomplete="off">            
            <div class="row">
                <div class="col-sm-8">
                    <div id="map-canvas" style="height: 500px;"></div>                   
                </div>
                <div class="col-sm-4">
                    @*<div id="directions-panel" style="height:500px;overflow:auto"></div>*@
                    <div class="table-responsive">
            <table id="CategorySystemTypeCodeMapping" class="table table-fixedheader table-bordered" style="border: 1px solid rgb(222, 218, 218);">
                <thead class="tableHeading">
                    <tr>
                        <th style="text-align: center;" width="40%">DateTime</th>
                        <th style="text-align: center;" width="30%">Latitude</th>
                        <th style="text-align: center;" width="30%">Longitude</th>

                    </tr>
                </thead>
                <tbody style="height:450px" id="TrackingdetailTableGrid">

                </tbody>
            </table>
        </div>
                </div>
            </div>
            
        </form>

    </div>


</div>

@section scripts{
    <script type="text/javascript" src="@Url.Content("~/Scripts/ng/Controllers/UM/TrackingTechnicianView.js")"></script>
}






