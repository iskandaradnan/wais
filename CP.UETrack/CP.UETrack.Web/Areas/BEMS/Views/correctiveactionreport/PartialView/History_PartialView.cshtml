            <div class="row" id="divHistory">
                <hr style="margin: 2em 0;" />
                <div class="col-sm-12">
                    <div class="table-responsive">
                        <table id="tableHistory" class="table table-fixedheader table-bordered">
                            <thead class="tableHeading">
                                <tr>
                                    <th style="text-align: center;" width="10%">CAR Status</th>
                                    <th style="text-align: center;" width="30%">Root Cause</th>
                                    <th style="text-align: center;" width="15%">Solution</th>
