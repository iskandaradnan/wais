            <div class="row mt20">
                <div class="form-group">
                    <div class="col-sm-12 text-center">
                        <button type="button" id="btnHistoryCancel" class="btn btn-grey customButton" title="Reset"><i class="fa fa-repeat" aria-hidden="true"></i>Reset</button>
                    </div>
                </div>
            </div>
