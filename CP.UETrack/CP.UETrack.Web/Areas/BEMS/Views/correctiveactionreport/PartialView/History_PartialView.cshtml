                        <div class="col-sm-6">
                            <input type="text" class="form-control" id="txtCARNumberHistory" disabled>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-6 control-label">Indicator</label>
                        <div class="col-sm-6">
                            <select class="form-control" id="selQAPIndicatorIdHistory" disabled>
                                <option value="null">Select</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-6 control-label">CAR Date</label>
                        <div class="col-sm-6">
                            <input type="text" id="txtCARDateHistory" class="form-control datatimeNoFuture" disabled/>
