
@{
    ViewBag.Title = "UETrack - Smart Assign Mapping";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<style>
    .green {
        background-color: #dff0d8;
    }

    .pink {
        background-color: #f2dede;
    }

    .buttonDisabled {
        background-color: #E0E0E0;
    }
</style>
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkqCkh3sb4lVPt4U-DHmnHej5AWYKbtIo&libraries=places"></script>

<div>

    @functions{
        public string GetAntiForgeryToken()
        {
            string cookieToken, formToken;
            AntiForgery.GetTokens(null, out cookieToken, out formToken);
            return cookieToken + ":" + formToken;
        }
    }

    <input id="antiForgeryToken" type="hidden" />
    <input type="hidden" id="primaryID" name="primaryID" value="@ViewBag.CurrentID" />
    <input type="hidden" id="ActionType" name="ActionType" value="@ViewBag.ActionType" />
    <input type="hidden" id="Timestamp" name="Timestamp" value="null" />

    <div class="content_header">
        <ol class="breadcrumb">
            <li><a href="/home/dashboard"><i class="fa fa-home"></i></a></li>
            <li>BEMS</li>
            <li class="active">Smart Assign Execution - Work Order</li>
            <a id="HelpDescriptionLink" data-toggle="modal" title="" tabindex="0" data-target="#Description" aria-disabled="false">
                <i class="fa fa-question-circle" aria-hidden="true" style="float:right;margin-right:20px;"></i>
                <span class="badge" id="helpdescriptionid"></span>
            </a>
        </ol>
        <h4>Smart Assign Execution - Work Order</h4>
        
    </div>  


    <div class="content_body">
        <form name="frmsmartassign" id="frmsmartassign" class="form-horizontal" novalidate style="display:none" autocomplete="off"> 

            <div class="row" style="margin-top:15px">
                <div class="col-sm-12">
                    <div class="col-sm-1">
                        <div class="input-group">
                            <input type="button" id="btnFetch" value="Assign" class="btn btn-primary" />
                        </div>
                    </div>
                </div>
            </div>
        </form>
        @Html.Partial("~/Areas/FEMS/Views/smartassigns/List.cshtml")
    </div>
</div>
@section scripts{
    <script type="text/javascript" src="@Url.Content("~/Scripts/ng/Controllers/UM/SmartAssign.js")"></script>
}



