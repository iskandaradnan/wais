
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    @functions{
        public string GetAntiForgeryToken()
        {
            string cookieToken, formToken;
            AntiForgery.GetTokens(null, out cookieToken, out formToken);
            return cookieToken + ":" + formToken;
        }
    }

    <input id="antiForgeryToken" type="hidden" />
    <input type="hidden" id="primaryID" name="primaryID" value="@ViewBag.CurrentID" />
    <input type="hidden" id="ActionType" name="ActionType" value="@ViewBag.ActionType" />
    <input type="hidden" id="Timestamp" name="Timestamp" value="null" />
    <input type="hidden" id="hdnStatus" name="hdnStatus" />
    <input type="hidden" id="hdnAttachId" name="HdnAttachId" />

    <!-- Content Header Starts -->
    <div class="content_header">
        <ol class="breadcrumb">
            <li>
                <a href="/home/dashboard"><i class="fa fa-home"></i></a>
            </li>
            <li>HWMS</li>
            <li>Master</li>
            <li class="active">Treatment Plant</li>
            <a id="HelpDescriptionLink" data-toggle="modal" title="" tabindex="0" data-target="#Description" aria-disabled="false">
                <i class="fa fa-question-circle" aria-hidden="true" style="float:right;margin-right:20px;"></i>
                <span class="badge" id="helpdescriptionid"></span>
            </a>
        </ol>
        <h4></h4>
      
    </div>

    <ul class="nav nav-tabs" role="tablist">
        <li class="active"><a href="#formppmplanner" data-toggle="tab">Treatment Plant</a></li>
        <li class=""><a data-toggle="tab" href="#tab-2">Vehicle Details</a></li>
        <li class=""><a data-toggle="tab" href="#tab-3">Driver Details</a></li>

    </ul>
    <!-- Content Header Ends -->
    <!-- Contents goes here -->
    
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="formppmplanner">
            <form id="formTreatmentPlant" name="formTreatmentPlant" class="form-horizontal">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Treatment Plant Code<span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="code">
                                    <input type="text" required class="form-control" id="txtTreatmentPlantCode" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <div class="form-group">
                                <label class="col-sm-6 control-label">Treatment Plant Name <span style="color:red">*</span></label>
                                <div>
                                    <div class="col-sm-6" id="plantname">
                                        <input type="text" required class="form-control" id="txtTreatmentPlantName" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Registration No. / License No.</label>
                            <div>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtRegistrationNo" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">

                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Address Line 1 <span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-9" id="address">
                                    <input type="text" required class="form-control" id="txtAdressLine1" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Address Line 2 </label>
                            <div>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="txtAdressLine2" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">City / Town<span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="city">
                                    <input type="text" required class="form-control" id="txtCity" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">State <span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6">
                                    <div id="state">
                                        <select required class="form-control" id="ddlState"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Post Code<span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="postcode">
                                    <input type="text" required class="form-control" id="txtPostsCode" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Ownership <span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6">
                                    <div id="ownership">
                                        <select required class="form-control" id="ddlOwnership"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Contact Number<span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="contactnumber">
                                    <input type="text" required class="form-control" id="txtContactNumber" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Fax Number</label>
                            <div>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtFaxNumber" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">DOE File No. <span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="doefile">
                                    <input type="text" required class="form-control" id="txtDOEFileNo" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Owner Name <span style="color:red">*</span></label>
                            <div>
                                <div class="col-sm-6" id="ownername">
                                    <input type="text" required class="form-control" id="txtOwnerName" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Number Of Store</label>
                            <div>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtNumberOfStore" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <label class="col-sm-6 control-label">Capacity Of Storage</label>
                            <div>
                                <div class="col-sm-6">
                                    <input type="text" class="form-control" id="txtCapacityOfStorage" autocomplete="off" name="AreaCodeNo" maxlength="25"  />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Remarks</label>
                            <div class="col-sm-9">
                                <div>
                                    <textarea id="txtRemarks" class="form-control" rows="2" cols="50" name="ScopeOfWork" maxlength="500"></textarea>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <hr />
                <h5><b>License Details</b> </h5>
                <br /><br />
                      <div class="row">
                          <div class="col-sm-12">
                              <div class="table-responsive">
                                  <table id="table1" class="table table-bordered">
                                      <thead class="tableHeading">
                                          <tr>
                                              <th width="3%" class="text-center">                                                 
                                                  <span style="text-align:center" class="delete-row glyphicon glyphicon-remove" id="deleteLicenseCode"></span>
                                              </th>
                                              <th style="text-align: center;" width="10%">License Code <span style="color:red">*</span></th>
                                              <th style="text-align: center;" width="10%">License Description </th>
                                              <th style="text-align: center;" width="10%">License No.<span style="color:red">*</span></th>
                                              <th style="text-align: center;" width="10%">Class / Grade / Capacity</th>
                                              <th style="text-align: center;" width="10%">Issue Date<span style="color:red">*</span></th>
                                              <th style="text-align: center;" width="10%">Expiry Date<span style="color:red">*</span> </th>

                                          </tr>
                                      </thead>
                                      <tbody id="tbodyLicenseCode">
                                          <tr id="Licensetablerow" >
                                              <td style="text-align:center">
                                                  <input type="hidden" id="hdnLicenseCodeId1" value="0" />
                                                  <input type="checkbox" id="isDelete1" name="isDelete" />
                                              </td>
                                              <td id="licensecode">
                                                  <input type="text" required class="form-control HWMSLicence" placeholder="Please Select" id="txtLicenseCode1" autocomplete="off" name="FailureType" maxlength="25"  />
                                                  <input type="hidden" id="hdnLicensecode1" />
                                                  <div class="col-sm-12" style="position:absolute; width:160px;" id="divLicenseFetch1"></div>
                                              </td>
                                              <td id="description">
                                                  <input type="text" class="form-control" id="txtLicenseDescription1" disabled="disabled" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="licenseno">
                                                  <input type="text" required class="form-control" id="txtLicenseNo1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="failuretype">
                                                  <input type="text" class="form-control" id="txtClass1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="issuedate">
                                                  <input type="text" required class="form-control datetime" id="txtIssueDate1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="expirydate">
                                                  <input type="text" required class="form-control datetime" id="txtExpiryDate1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                          </tr>
                                      </tbody>
                                  </table>
                              </div>
                              </div>
                          </div>
                          <br />
                          <div class="row mt20" id="AttachRowPlus">
                              <div class="col-sm-3 ">
                                  <div class="form-group">
                                      <div class="col-sm-3">
                                          <div>
                                              <a class="btn btn-primary" id="addLicenseCode">+</a>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                          <hr />
                          <h5><b>Disposal Type</b> </h5>
                          <br /><br />

                          <div class="tab-content">
                              <div role="tabpanel" class="tab-pane active" id="tabCleansingDetails">
                                  <table id="table2" class="table table-bordered">
                                      <thead class="tableHeading">
                                          <tr>
                                              <th width="3%" class="text-center">                                                  
                                                  <span style="text-align:center" class="delete-row glyphicon glyphicon-remove" id="deleteDisposalType"></span>
                                              </th>
                                              <th style="text-align: center;" width="10%">Method of Disposal <span style="color:red">*</span></th>
                                              <th style="text-align: center;" width="10%">Status <span style="color:red">*</span></th>
                                              <th style="text-align: center;" width="10%">Design Capacity</th>
                                              <th style="text-align: center;" width="10%">Licensed Capacity</th>
                                              <th style="text-align: center;" width="10%">Unit<span style="color:red">*</span></th>
                                          </tr>
                                      </thead>
                                      <tbody id="tbodyDisposalType">
                                          <tr id="DriverRow">
                                              <td style="text-align:center">
                                                  <input type="hidden" id="hdnDisposalTypeId1" value="0" />
                                                  <input type="checkbox" id="isDelete1" name="isDelete" />
                                              </td>
                                              <td id="disposal">
                                                  <select required class="form-control" id="ddlMethodofDisposal1"></select>
                                              </td>
                                              <td id="status">
                                                  <select required class="form-control" id="ddlStatus1"></select>
                                              </td>
                                              <td id="type">
                                                  <input type="text" class="form-control" id="txtDesignCapacity1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="capacity">
                                                  <input type="text" class="form-control" id="txtLicensedCapacity1" autocomplete="off" name="FailureType" maxlength="25"  />
                                              </td>
                                              <td id="unit">
                                                  <select required class="form-control" id="ddlUnit1"></select>
                                              </td>
                                          </tr>
                                      </tbody>
                                  </table>
                              </div>
                          </div>

                          <div class="row mt20" id="AttachRowPlus">
                              <div class="col-sm-3 ">
                                  <div class="form-group">
                                      <div class="col-sm-3">
                                          <div>
                                              <a class="btn btn-primary" id="addDisposalType">+</a>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>

                          <div id="errorMsg" class="errormsgcenter" style="color:red; visibility:hidden;" aria-hidden="true">
                              <!-- ngIf: RescheduleErrorList.length > 0 -->
                          </div>

                          <div class="row mt30">
                              <div class="form-group">
                                  <div class="col-sm-12 text-center">
                                      <input type="button" id="btnSave" class="btn btn-primary" value="Save">
                                      <input type="button" id="btnSaveandAddNew" class="btn btn-primary" value="Save & Add New" />
                                      <input type="button" id="btnCancel" class="btn btn-primary" value="Reset">
                                      @*<button type="button" id="btnSave" class="btn btn-primary customButton" title="Save" tabindex="5"><i class="fa fa-floppy-o" aria-hidden="true"></i>Save</button>
                                        <button type="button" id="btnSaveandAddNew" class="btn btn-primary customButton" title="Saveandnew" tabindex="5"><i class="fa fa-plus" aria-hidden="true"></i>Save&Addnew</button>
                                        <button type="button" id="btnCancel" name="btnCancel" class="btn btn-grey customButton" title="Reset"><i class="fa fa-repeat" aria-hidden="true"></i>Reset</button>*@
                                       </div>
                              </div>
                          </div>
                          <label id="TreatmentPlantStatus" style="font-size:14px"></label>
                        </form>

        </div>

        @*Vichile Details Tab...........*@

        <div role="tabpanel" class="tab-pane" id="tab-2">
            <div class="content_body">
                @Html.Partial("~/Areas/HWMS/Views/TreatmentPlant/VichileDetails.cshtml")
            </div>
           
        </div>

        @*Driver Details Tab.....*@

        <div role="tabpanel" class="tab-pane" id="tab-3">
            <div class="content_body">
                @Html.Partial("~/Areas/HWMS/Views/TreatmentPlant/DriverDetails.cshtml")
            </div>
           
        </div>

    </div>
    @Html.Partial("~/Areas/HWMS/Views/TreatmentPlant/List.cshtml")
</div>
@section scripts{
    <script type="text/javascript" src="@Url.Content("~/Scripts/ng/controllers/HWMS/TreatmentPlant.js")"></script>
}





